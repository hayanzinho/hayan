lado1=float(input("Informe o lado1: "))
lado2=float(input("Informe o lado2: "))
lado3=float(input("Informe o lado3: "))

if lado1+lado2>lado3: 
     lado1==lado2==lado3
     print("Triangulo Equilatero!")
elif lado1==lado2 or lado2==lado3 or lado1==lado2:
    print("Isóceles")
elif lado1!=lado2!=lado3:
    print("Escaleno")
else:
    print("Não pode se formar um triangulo!")
valor=int(input("Digite o valor: "))
cem=int(valor/100)
valor=valor-(cem*100)
cinq=int(valor/50)
valor=valor-(cinq*50)
den=int(valor/10)
valor=valor-(den*10)
cin=int(valor/5)
valor=valor-(cin*5)
un=int(valor/1)
valor=valor-(un*1)
print("nota de 100:",cem)
print("nota de 50:",cinq)
print("Nota de 10:",den)
print("Nota de 5:",cin)
print("Nota de 1:",un)